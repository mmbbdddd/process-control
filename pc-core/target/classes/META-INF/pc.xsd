<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://ddbm.io/pc.xsd"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="flow" type="pc:flowType" xmlns:pc="http://ddbm.io/pc.xsd"/>
    <xs:complexType name="onType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="event" use="optional"/>
                <xs:attribute type="xs:string" name="action" use="optional"/>
                <xs:attribute type="xs:string" name="to" use="optional"/>
                <xs:attribute type="xs:string" name="router" use="optional"/>
                <xs:attribute type="xs:string" name="retryCount" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="startType">
        <xs:sequence>
            <xs:element type="pc:onType" name="on" xmlns:pc="http://ddbm.io/pc.xsd"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="nodeType">
        <xs:sequence>
            <xs:element type="pc:onType" name="on" xmlns:pc="http://ddbm.io/pc.xsd"/>
        </xs:sequence>
        <xs:attribute type="xs:string" name="name" use="optional"/>
    </xs:complexType>
    <xs:complexType name="endType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="name" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="caseType">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="test" use="optional"/>
                <xs:attribute type="xs:string" name="to" use="optional"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="routerType">
        <xs:sequence>
            <xs:element type="pc:caseType" name="case" maxOccurs="unbounded" minOccurs="0"
                        xmlns:pc="http://ddbm.io/pc.xsd">
                <xs:annotation>
                    <xs:documentation>你好</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" name="name"/>
    </xs:complexType>
    <xs:complexType name="routersType">
        <xs:sequence>
            <xs:element type="pc:routerType" name="router" xmlns:pc="http://ddbm.io/pc.xsd"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="flowType">
        <xs:sequence>
            <xs:element type="pc:startType" name="start" xmlns:pc="http://ddbm.io/pc.xsd"/>
            <xs:element type="pc:nodeType" name="node" maxOccurs="unbounded" minOccurs="0"
                        xmlns:pc="http://ddbm.io/pc.xsd"/>
            <xs:element type="pc:endType" name="end" maxOccurs="unbounded" minOccurs="0"
                        xmlns:pc="http://ddbm.io/pc.xsd"/>
            <xs:element type="pc:routersType" name="routers" xmlns:pc="http://ddbm.io/pc.xsd"/>
            <xs:element type="xs:string" name="plugins">
                <xs:annotation>
                    <xs:documentation>&lt;plugin name="catPlugin"/> &lt;plugin name="sentinelPlugin"/>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" name="name"/>
    </xs:complexType>
</xs:schema>